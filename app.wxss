/*用户购买信息  */

.userBuy {
  position: absolute;
  top: 38rpx;
  left: 20rpx;
  display: flex;
  align-items: center;
  padding: 12rpx 0;
  box-sizing: content-box;
  background-color: rgba(0, 0, 0, 0.8);
  opacity: 0;
  animation: opci 9s infinite;
  height: 40rpx;
  border-radius: 12rpx;
  padding-left: 10rpx;
}

@keyframes opci {
  0% {
    opacity: 0;
  }

  50% {
    opacity: 0.9;
  }

  80% {
    opacity: 0.9;
  }

  100% {
    opacity: 0;
  }
}

.userBuy .buyImg {
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  margin-left: 22rpx;
}

.userBuy .buyImg image {
