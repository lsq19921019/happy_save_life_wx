/* 列表顶部加载栏 */

.topLoading {
  height: 80rpx;
  width: 100%;
  background: #fff;
  text-align: center;
  padding-top: 14rpx;
}

.topLoading i {
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  display: inline-block;
  margin-right: 14rpx;
}

.topLoading i:nth-child(1) {
  animation: a3 0.8s infinite;
}

.topLoading i:nth-child(2) {
  animation: a2 0.8s infinite;
}

.topLoading i:nth-child(3) {
  animation: a1 0.8s infinite;
}

@keyframes a1 {
  0% {
    background: #999;
  }

  50% {
    background: #ccc;
  }

  100% {
    background: #e5e5e5;
  }
}

@keyframes a2 {
  0% {
    background: #ccc;
  }

  50% {
    background: #e5e5e5;
  }

  100% {
    background: #999;
  }
}

@keyframes a3 {
  0% {
    background: #e5e5e5;
  }

  50% {
    background: #999;
  }

  100% {
    background: #ccc;
  }
}

