/* 清除buttom默认样式 */

.home image {
  display: inline-block;
}

.home form button {
  overflow: visible;
  border-radius: 0;
  margin-left: 0;
  margin-right: 0;
  padding-left: 0;
  padding-right: 0;
  border: none !important;
}

/*join S */

.join {
  margin: 20rpx 30rpx 0;
  background-color: #fff;
  border-radius: 12rpx;
  display: flex;
}

.join>button {
  flex: 1;
  margin: 16px 0 18px 0;
  text-align: center;
}

.join>button:nth-child(2) {
  border-left: 1rpx solid #ccc !important;
  border-right: 1rpx solid #ccc !important;
}

.join>button image {
  width: 64rpx;
  height: 60rpx;
  margin-top: -22rpx;
}

.join>button p {
  text-align: center;
  font-size: 26rpx;
  color: #333;
  line-height: 16rpx;
}

/*join E  */

.head {
  width: 100%;
  height: 270rpx;
  position: relative;
  color: #fff;
  background: #ff2b33;
}

.head  .head_bg {
  position: relative;
}

.head  .head_bg .personalMsg {
  padding-top: 46rpx;
  position: relative;
  line-height: 55rpx;
  height: 196rpx;
}

.head  .head_bg .personalMsg .avatar {
  width: 104rpx;
  height: 104rpx;
  border-radius: 50%;
  margin-left: 52rpx;
  margin-right: 20rpx;
  float: left;
  position: relative;
}

.personalMsg .avatar .img {
  width: 104rpx;
  height: 104rpx;
  display: block;
  border-radius: 50%;
  overflow: hidden;
}

.personalMsg .avatar .role {
  width: 120rpx;
  height: 36rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 18rpx;
  position: absolute;
  bottom: -10rpx;
  left: -8rpx;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  color: rgba(255, 43, 51, 1);
  font-size: 22rpx;
}

.personalMsg .avatar .role .roleico {
  width: 22rpx;
  height: 22rpx;
  display: inline-block;
  margin: 0 10rpx;
}

.personalMsg  .nameMsg {
  float: left;
  text-align: left;
  font-size: 32rpx;
}

.personalMsg  .nameMsg .id {
  font-size: 20rpx;
}

.personalMsg  .nameMsg .id .copy {
  border: 2rpx solid rgba(246, 245, 245, 1);
  border-radius: 4rpx;
  font-size: 20rpx;
  padding: 5rpx 10rpx;
  margin-left: 10rpx;
}

.head  .head_bg .personalMsg .setp {
  width: 196rpx;
  height: 48rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 24rpx 0px 0px 24rpx;
  position: absolute;
  right: 0;
  top: 42rpx;
  color: rgba(255, 43, 51, 1);
  padding-left: 20rpx;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
}

.head  .head_bg .personalMsg .setp .setpico {
  width: 25rpx;
  height: 23rpx;
}

.head  .head_bg .personalMsg .setp .arrow {
  width: 13rpx;
  height: 22rpx;
}

.head  .head_bg .personalMsg .setp text {
  margin: 0 10rpx 0 10rpx;
}

/* /提现金额------------------------------------- */

.cock {
  width: 690rpx;
  position: absolute;
  top: 185rpx;
  left: 0;
  right: 0;
  margin: auto;
}

.userMoney {
  width: 690rpx;
  height: 260rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
}

.userMoney .canMoney {
  width: 100%;
  height: 90rpx;
  border-bottom: 2rpx solid #f5f5f5;
  color: #010101;
  line-height: 90rpx;
  padding-left: 25rpx;
  position: relative;
  font-size: 22rpx;
}

.userMoney .canMoney  .mdetails {
  color: #ff2b33;
  font-family: FZLTCHK--GBK1-0;
  padding-left: 10rpx;
  font-size: 40rpx;
}

.userMoney .canMoney .deposit {
  width: 130rpx;
  height: 56rpx;
  background: rgba(255, 43, 51, 1);
  border-radius: 28rpx;
  color: #fff;
  font-size: 30rpx;
  line-height: 56rpx;
  text-align: center;
  position: absolute;
  top: 50%;
  right: 20rpx;
  transform: translateY(-50%);
}

.userMoney .total {
  display: flex;
  height: 160rpx;
  width: 100%;
  align-items: center;
  justify-content: space-around;
}

.userMoney .total .totalItem {
  width: 25%;
  height: 40%;
  border-right: 2rpx solid #f5f5f5;
  color: black;
}

.userMoney .total .totalItem .itemType {
  text-align: center;
  font-size: 24rpx;
  color: #010101;
  font-family: FZLTCHK--GBK1-0;
  font-weight: bold;
}

.userMoney .total .totalItem .itemNum {
  text-align: center;
  font-size: 22rpx;
  color: #010101;
  padding-top: 15rpx;
}

.Customer_service {
  display: block;
  position: fixed;
  padding: 0;
  width: 86rpx;
  height: 86rpx;
  right: 40rpx;
  bottom: 160rpx;
  outline: none;
  border-radius: 50%;
  border: 1px solid transparent;
  margin: 0;
}

button::after {
  border: 0;
}

.Customer_service image {
  width: 86rpx;
  height: 86rpx;
}

/* 列表 */

.strip {
  position: relative;
  overflow: hidden;
  min-height: 170rpx;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
}

.strip.after {
  margin-top: 200rpx;
}

.strip.before {
  /* margin-top: 120rpx; */
}

.strip .wrap {
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  width: 100%;
  background: #fff;
}

.strip .common {
  width: 25%;
  height: 160rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  text-align: center;
  background: #fff;
}

.strip .common image {
  width: 40rpx;
  height: 40rpx;
}

.strip .common text {
  color: #010101;
  font-size: 24rpx;
}

.strip  .list_box {
  width: 100%;
}

.strip  .list_box .list {
  margin-top: 20rpx;
  height: 100rpx;
  padding: 0 32rpx;
  line-height: 100rpx;
  background: #fff;
  border-bottom: 2rpx solid #f5f5f5 !important;
  text-align: left;
  background: #fff url("data:image/png;base64,") no-repeat 95% center;
  background-size: 12rpx 24rpx;
}

.strip  .list_box .list .listico {
  width: 44rpx;
  height: 44rpx;
  margin-right: 25rpx;
  vertical-align: middle;
}

.freemask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.7);
  overflow: hidden;
  z-index: 9999;
  color: #fff;
}

.maskconcent {
  width: 600rpx;
  height: 412rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 10rpx;
  z-index: 10000;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  padding: 0;
  overflow: hidden;
}

.maskconcent .tittle {
  font-size: 30rpx;
  font-family: FZLTHK--GBK1-0;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  margin-top: 40rpx;
  text-align: center;
}

.maskconcent .futi {
  width: 490rpx;
  font-size: 30rpx;
  font-family: FZLTHK--GBK1-0;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-align: left;
  margin: 30rpx auto 20rpx;
}

.inputbox {
  width: 490rpx;
  height: 80rpx;
  background: rgba(243, 243, 243, 1);
  margin: 0 auto;
}

.inputbox input {
  width: 490rpx;
  height: 80rpx;
  padding-left: 30rpx;
}

.maskconcent .bottom {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-top: 1px solid rgba(217, 217, 217, 1);
  position: absolute;
  bottom: 0;
  font-size: 30rpx;
  font-family: FZLTHK--GBK1-0;
  color: rgba(153, 153, 153, 1);
  text-align: center;
  /* background: pink; */
  margin: 0;
}

.bottom view {
  width: 50%;
  height: 100%;
}

.bottom .left {
  border-right: 1px solid rgba(217, 217, 217, 1);
}

.bottom .right {
  color: rgba(255, 43, 51, 1);
}

