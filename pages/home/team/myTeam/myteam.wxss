/* 索搜框 */

.seekbg {
  height: 200rpx;
  width: 100%;
  background: #f86402;
  padding-top: 30rpx;
  margin-bottom: 80rpx;
}

.seekbg .seekBox {
  background: #ececec;
  width: 570rpx;
  height: 72rpx;
  position: relative;
  border-radius: 50rpx;
  overflow: hidden;
  margin: 0 auto;
}

.Oactive {
  background-color: #bab9b9;
  height: 100%;
}

.seekbg .seekBox input {
  width: 440rpx;
  height: 72rpx;
  line-height: 72rpx;
  color: #bab9b9;
  font-size: 28rpx;
  margin-left: 40rpx;
}

.seekbg .seekBox .definite {
  background: #fff;
  position: absolute;
  height: 100%;
  width: 120rpx;
  line-height: 64rpx;
  text-align: center;
  top: 0;
  right: 0rpx;
}

.seekbg .seekBox .definite image {
  width: 44rpx;
}

.team {
