/* 团队人员 */

.layer {
  font-size: 26rpx;
  color: #666;
  background: #fff;
  border-bottom: 2rpx solid #e9e9e9;
  width: 100%;
  padding: 14rpx 30rpx 14rpx;
  height: 142rpx;
}

.layer>view {
  float: left;
}

/* 头像 */

.layer .avatar {
  margin-right: 30rpx;
}

.layer .avatar image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 14rpx;
}

/* 用户名 */

.layer .userName {
  font-size: 20rpx;
  margin-right: 30rpx;
  width: 24%;
}

.layer .userName p:nth-child(1) {
  font-size: 28rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.layer .userName p:nth-child(2) {
  border: 1rpx solid rgba(240, 140, 166, 0.5);
  border-radius: 40rpx;
  display: inline-block;
  padding: 0rpx 16rpx;
  line-height: 28rpx;
  margin: 5rpx 0 4rpx;
}

.layer .userName p:nth-child(3) {
  font-size: 20rpx;
}

/*人数 */

.layer .people {
  overflow: hidden;
  margin-right: 24rpx;
}

.layer .people p {
  line-height: 54rpx;
  width: 196rpx;
  overflow: hidden;
  white-space: normal;
}

/* 加好友 */

