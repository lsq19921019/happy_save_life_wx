  text-align: left;
  height: auto;
  width: 620rpx;
  position: absolute;
  top: 50%;
  /* top: 30%; */
  left: 50%;
  transform: translate(-50%,-50%);
  background-color: white;
  border-radius: 12rpx;
}
.withdraw .drawType .drawHead{
   text-align: center;
  font-size: 36rpx;
