/* 余额 */

.profit .balance {
  width: 100%;
  height: 360rpx;
  padding: 40rpx 30rpx 0;
  color: #fff;
  position: relative;
}

.profit .balance .noneBalance {
  font-size: 54rpx;
  text-align: center;
  margin-top: 80rpx;
  color: rgba(255, 255, 255, 0.8);
}

.profit .balance .nonewallet {
  text-align: center;
  font-size: 28rpx;
  margin-top: 20rpx;
  color: rgba(255, 255, 255, 0.8);
}

.balance .money {
  float: left;
}

.balance .money p:first-child {
  font-size: 34rpx;
  margin-left: 10rpx;
  padding-bottom: 30rpx;
}

.balance .money p:last-child {
  font-size: 52rpx;
}

.balance  .withdrawals {
  font-size: 30rpx;
  color: rgba(255, 255, 255, 0.8);
  float: right;
  border: 2rpx solid rgba(255, 255, 255, 0.8);
  padding: 6rpx 28rpx;
  border-radius: 50rpx;
  position: relative;
  top: 30rpx;
  line-height: 32rpx;
  background: none;
}

.color {
  color: red;
}

.proItem {
  width: 33%;
  position: relative;
  text-align: center;
}

.Item {
  position: absolute;
  bottom: -35rpx;
  text-align: center;
  left: 50%;
  transform: translateX(-50%);
  width: 100%;
}

.writeColor {
  color: #e84d74;
}

.order .box .date p {
  float: left;
  width: 50%;
  text-align: center;
  box-sizing: border-box;
}

.order .box .date p:first-child {
  border-right: 2rpx solid #dedede;
}

.order .box>p:nth-child(2) {
  font-size: 28rpx;
  color: #333;
  padding-bottom: 38rpx;
}

.order .box .number view {
  float: left;
  font-size: 24rpx;
  width: 33.3%;
}

.order .box .number view:first-child {
  float: left;
}

.order .box .number view p:nth-child(1) {
  font-size: 24rpx;
  color: #333;
  padding-bottom: 22rpx;
}

.order .box .number view p:nth-child(2) {
  font-size: 28rpx;
  color: #333;
  font-weight: 600;
}

