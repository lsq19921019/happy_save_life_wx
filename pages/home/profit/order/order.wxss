/*提示框  */
.tipBg{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, .3);
  z-index: 999;
}
.tipBg .tooltip{
  height: 340rpx;
  width: 460rpx;
  background-color:white;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  border-radius: 12rpx;
}
.tipBg .tooltip .tit{
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  display: block;
  padding: 30rpx 0 50rpx 0;
}
.tipBg .tooltip .conTit{
  padding: 0 30rpx;
  text-align: center;
  font-size: 20rpx;
}
.tipBg .tooltip  .comeon{
   padding:  30rpx 0;
  text-align: center;
  font-size: 20rpx;
}
.tipBg .tooltip .iknow{
  height: 100rpx;
  width: 100%;
  border-top: 2rpx solid #F5F5F5;
  position: absolute;
  bottom: 0;
