/* 商品例表 */

.orderGoods .goodsbox {
  padding: 0rpx 20rpx;
  margin-bottom: 16rpx;
  font-size: 26rpx;
  height: 200rpx;
  position: relative;
  background: #fff;
}
.noget{
  background-color: #f5f5f5;
}
.orderGoods .goodsbox .img {
  width: 150rpx;
  height: 150rpx;
}

.orderGoods .goodsbox .info {
  overflow: hidden;
  font-size: 26rpx;
}

.orderGoods .goodsbox .info .viceTitle {
  font-size: 26rpx;
  color: #606060;
  height: 43rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
   padding-top: 10rpx; 
  padding-left: 10rpx;
  display: inline-block;
  width: 90%;
  vertical-align: middle;
}
.orderGoods .goodsbox .info .spa {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
  position: relative;
  top:20rpx;
}
.orderGoods .goodsbox .info .commission {
  padding-bottom: 16rpx;
  font-size: 20rpx;
  color: #909090;
  position: relative;
}

