.page {
  padding: 0;
  margin: 0;
}
button {
  font-size: 24rpx;
  background-color: #fff;
  border: none;
  margin: 0;
  line-height: 1;
}
.txt{
  white-space:nowrap;
}

button::after {
  border: none;
}
.page.fixed {
  overflow-y: hidden;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  overflow: hidden;
  overflow-y: hidden;
  z-index: 99;
}

.floor1 {
  position: relative;
}

image {
  width: 100%;
  height: 100%;
}

.seach {
  width: 750rpx;
  height: 90rpx;
  background: rgba(255, 43, 51, 1);
  position: relative;
  z-index: 1000;
  display: flex;
  /* justify-content: center; */
  justify-content: left;
  box-sizing: border-box;
  padding-left: 20rpx;
  align-items: center;
}

.inputbox {
  width: 652rpx;
  height: 60rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
  position: relative;
}

.seach_ico {
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  right: 25rpx;
}
.clear_ico {
  width: 38rpx;
  height: 38rpx;
  position: absolute;
  right: 30rpx;
}

.inputbox input {
  width: 100%;
  height: 100%;
  padding-left: 30rpx;
  padding-right: 70rpx;
}

/* 分类 分类 分类 分类 分类 分类 分类 分类 分类 分类 分类 */

.navBar {
  width: 100%;
  height: 96rpx;
  background: rgba(255, 255, 255, 1);
  border-bottom: 1rpx solid #f0f0f0;
  box-sizing: border-box;
  padding-left: 10rpx;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  position: relative;
  z-index: 1000;
}

.navBar .slide_view {
  width: 670rpx;
  height: 100%;
  white-space: nowrap;
  position: relative;
  box-sizing: border-box;
  padding: 0;
  margin: 0;
  padding-top: 20rpx;
  display: flex;
  flex-wrap: nowrap;
  /* background: pink; */
}

.swiperScroll {
  /* background: pink; */
  margin: 0;
  padding: 0 20rpx;
  padding-right: 30rpx;
  text-align: center;
  height: 56rpx;
  line-height: 56rpx;
  position: relative;
  display: inline-block;
  font-size: 28rpx;
  font-weight: bold;
  border-radius: 4rpx;
}

.navBar .slide_view>view:last-child {
  margin-right: 20rpx;
}

.slide_view .navItem {
  margin-left: 60rpx;
}

.slide_view .navItem::after {
  content: "";
  top: 50%;
  right: -20rpx;
  position: absolute;
  transform: translateY(-50%);
  height: 60%;
  border-right: 2rpx solid #909090;
}

.navBar .up {
  flex: 1;
  height: 96rpx;
  margin-left: 10rpx;
  /* background: red; */
  display: flex;
  align-items: center;
}

.navBar .up_ico {
  width: 29rpx;
  height: 17rpx;
}

.swiperScroll.active {
  background: rgba(255, 43, 51, 1);
  color: #fff;
  text-align: center;
}

.pop-up {
  width: 100%;
  position: absolute;
  top: 96rpx;
  left: 0;
  background: rgba(255, 255, 255, 1);
  padding: 10rpx 35rpx 30rpx 35rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: flex-start;
  border-top: 1rpx solid #f0f0f0;
}

.pop {
  width: 154rpx;
  height: 54rpx;
  background: rgba(248, 248, 248, 1);
  border-radius: 4rpx;
  text-align: center;
  line-height: 54rpx;
  margin-right: 21rpx;
  margin-top: 15rpx;
}

.pop.active {
  background: rgba(255, 239, 240, 1);
  color: rgba(255, 43, 51, 1);
}

.pop:nth-of-type(4n) {
  margin-right: 0;
}

/* 轮播图+开团信息 -----------------------------------------------------*/

.bannerAD {
  position: relative;
}

.bannerAD .swiper {
  width: 750rpx;
