/* nav导航 -----------------------------------------------------------------------------------*/

.navlist {
  width: 100%;
  background: rgba(255, 255, 255, 1);
  /* margin: 10rpx 0; */
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
