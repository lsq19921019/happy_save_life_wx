image {
  width: 100%;
  height: 100%;
}

.banner {
  width: 100%;
  height: 194rpx;
  background: rgba(246, 245, 245, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.des text{
  height: 0;
  display: block;
