}

/* 111111111111111111111111111111111111111111111111111111111111 */

.imgMark {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 21;
  text-align: center;
}

.imgMark image {
  width: 60%;
  height: 60%;
  position: absolute;
  left: 50%;
  top: 70rpx;
  transform: translate(-50%, 0);
}

.imgMark>view {
  color: #fff;
  position: absolute;
  top: 71%;
  width: 100%;
  font-size: 14px;
}

.imgMark>.text_t {
  top: 76%;
}

.imgMark .bottom {
  width: 300rpx;
  position: absolute;
  top: 82%;
  left: 50%;
  transform: translate(-50%, 0);
  text-align: center;
}

.imgMark .bottom .text {
  color: #fff;
  text-decoration: underline;
}
