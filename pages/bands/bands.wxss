}

.navbox {
  width: 750rpx;
  height: 90rpx;
  background: rgba(255, 255, 255, 1);
  font-family: FZLTHK--GBK1-0;
  display: flex;
  flex-wrap: nowrap;
  margin-bottom: 10rpx;
}

.nav {
  width: 50%;
  height: 100%;
  text-align: center;
  line-height: 90rpx;
  font-size: 28rpx;
  color: rgba(0, 0, 0, 1);
}

.nav.active {
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(255, 43, 51, 1);
}

.circlewrap {
}

.circlelist {
  width: 100%;
  min-height: 100rpx;
  height: auto;
  padding: 34rpx 44rpx 34rpx 24rpx;
  background: #fff;
  margin-bottom: 10rpx;
  position: relative;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-end;
}

.circlelist .hot {
  position: absolute;
  top: 0;
  left: 0;
  width: 70rpx;
  height: 70rpx;
}

/* 顶部 */

.circlelist .top {
  width: 100%;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

/* 内容区域 */

.circlelist .concent {
  width: 584rpx;
}

.top .left {
}

.top .left image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.top .right {
  /* background: pink; */
  font-size: 33rpx;
  color: #333;
  display: flex;
  flex: 1;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

.top .right .left p {
  font-size: 22rpx;
  font-family: Adobe Heiti Std R;
  color: rgba(153, 153, 153, 1);
}

.top .right .left .username {
  font-size: 30rpx;
  color: rgba(0, 0, 0, 1);
}

.top  .sharbtn {
  width: 140rpx;
  height: 52rpx;
  display: block;
  padding: 0;
  line-height: 52rpx;
  background: rgba(255, 43, 51, 1);
  border-radius: 26px;
  color: #fff;
  font-size: 30rpx;
  overflow: hidden;
}

.top  .sharbtn1 {
  width: 174rpx;
  height: 52rpx;
  display: block;
  padding: 0;
  line-height: 52rpx;
  border: 2rpx solid rgba(255, 43, 51, 1);
  border-radius: 26rpx;
  color: rgba(255, 43, 51, 1);
  font-size: 22rpx;
  overflow: hidden;
}


.sharbtn1 .center {
  height: 52rpx;
  padding: 0 20rpx;
  line-height: 52rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

.sharbtn1 .center image {
  width: 27rpx;
  height: 27rpx;
  margin-right: 10rpx;
}

.sharbtn1 .center text {
  height: 100%;
  line-height: 48rpx;
}

button::after {
  border: 0;
}

.sharbtn .center {
  height: 52rpx;
  padding: 0 20rpx;
  line-height: 52rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}

.sharbtn .center image {
  width: 27rpx;
  height: 28rpx;
}

.sharbtn .center text {
  height: 100%;
  line-height: 48rpx;
}

.des {
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  font-size: 33rpx;
  color: #646464;
}

.img_wrap {
  width: 584rpx;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
}

.img_wrap .img {
  width: 188rpx;
  height: 188rpx;
  position: relative;
  margin-bottom: 20rpx;
  margin-right: 10rpx;
}

.img_wrap .img:nth-of-type(3n) {
  margin-right: 0;
}

.img_wrap .img image {
  width: 188rpx;
  height: 188rpx;
}

.img_wrap .img .mask {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 70rpx;
  background: rgba(0, 0, 0, 0.3);
  color: #fff;
  padding: 5rpx 20rpx;
  font-size: 20rpx;
  /* display: none; */
}

.img_wrap .img .mask p {
  line-height: 32rpx;
}

.img_wrap .img .mask p:last-of-type {
  font-weight: bold;
}

.concent .bottom {
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-end;
  margin-top: 28rpx;
}

.concent .bottom .l_btn, .concent .bottom .r_btn {
  /* width: 188rpx;
  height: 52rpx; 
   border-radius: 26rpx;*/
  width: 200rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
}

.l_btn {
  background: rgba(255, 239, 240, 1);
  color: #ff2b33;
}

.r_btn {
  background: rgba(26, 174, 101, 1);
  color: #fff;
  margin-left: 20rpx;
}

.bottom .left {
  color: #ed5b20;
}

.bottom .right {
  color: #989898;
}

.copyconcent {
  padding: 10rpx 35rpx 10rpx 15rpx;
  width: 540rpx;
  height: 108rpx;
  background: rgba(245, 245, 246, 1);
  border-radius: 4rpx;
  font-size: 28rpx;
  color: #5c5c5c;
  margin-top: 40rpx;
  position: relative;
}

.copyconcent .copy {
  width: 80rpx;
  height: 80rpx;
  background: rgba(255, 163, 167, 1);
  position: absolute;
  right: -50rpx;
  top: 14rpx;
  color: #fff;
  text-align: center;
}

.bottom .copy {
  width: 148rpx;
  height: 52rpx;
  line-height: 52rpx;
  background: rgba(255, 84, 15, 1);
  border-radius: 26rpx;
  color: #fff;
  text-align: center;
}

