
/*仅显示有券  */

.discounts{
  margin-top: 10rpx;
  height: 60rpx;
  width: 100%;
  line-height: 60rpx;
  border-bottom: 2rpx solid #f5f5f5;
  border-top: 2rpx solid #f5f5f5;
}
.commList{
  margin-top: 65rpx;
}
.disImg{
  display: inline-block;
  height: 22rpx;
  width: 40rpx;
  padding-left:30rpx; 
}
.disImg image{
  height: 100%;
  width: 100%;
}

.disText{
  padding-left: 25rpx;
  color: #909090;
  line-height: 60rpx;
}
.switch{
   transform: scale(0.6);
   float: right;
   vertical-align: middle;
   margin-top: -5rpx;
}
/*  */
.seekResult .seekFixed {
  background: #fff;
  width:100%;
}
.seekResult .topLoading{
  width:100%;
  height:138rpx;
  padding-top:80rpx;
}
.seekResult .seekbox {
  height: 80rpx;
  position: relative;
  margin: 0 auto;
  display: flex;
  line-height: 60rpx;
  padding: 15rpx 30rpx;
}

.seekResult  .seekbox .seekbug{
  width:28rpx;
  height:28rpx;
  position:absolute;
  left:48rpx;
  top:30rpx;
  z-index:20;
  background:url("data:image/png;base64,") no-repeat; 
  background-size: 28rpx 28rpx;
}

.seekResult  .seekbox input {
  position: relative;
  height: 60rpx;
  width: 500rpx;
  line-height: 60rpx;
  padding: 0 60rpx 0 64rpx;
  background: #f2f2f2;
    border-radius:100rpx;
}

.seekResult .seekbox .delete {
  position: absolute;
  width: 34rpx;
  height: 34rpx;
  right: 18rpx;
  top: 14rpx;
  z-index: 20;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 34rpx;
}

.seekResult .seekbox .seekGo {
  width: 96rpx;
  height: 54rpx;
  margin-top: 2rpx;
  margin-left: 20rpx;
  line-height: 54rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size:96rpx 54rpx;
  border-radius: 100rpx;
  color: #fff;
  text-align: center;
}
