/* 商品信息 */

.style1 .goodlist .info {
  height: 270rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  position: relative;
}

/* 商品名 */

.style1 .goodlist .info .name {
  font-size: 26rpx;
  color: rgba(0, 0, 0, 1);
  overflow: hidden;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
}

.style1 .goodlist .info .name  .baokuan {
  width: 64rpx;
  height: 36rpx;
  background: linear-gradient(-90deg, rgba(255, 43, 51, 1), rgba(255, 94, 23, 1));
  border-radius: 4rpx;
  display: inline-block;
  color: #fff;
  text-align: center;
  line-height: 36rpx;
  font-size: 22rpx;
}

/* 商品平台以及券额 */

.style1 .goodlist .info .bto {
  width: 100%;
  height: 165rpx;
  /* background: pink;  */
}

.style1 .goodlist .info .bto .left {
}

/* 商品平台以及券额 */
.style1 .goodlist .info .bto .top {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  margin-top: 16rpx;
}

.style1 .goodlist .info .bto .top .store,
.style2 .goodlist .info .bto .top .store {
  /* width: 36rpx;
  height: 36rpx; */
  width: 24rpx;
  height: 22rpx;
  margin-right: 20rpx;
}

.style1 .goodlist .info .bto  .top .storename,
.style2 .goodlist .info .bto  .top .storename {
  color: #999;
  font-size: 22rpx;
}


/* 商品销量 */
.style1 .goodlist .info .bto .left .sale {
  font-size: 22rpx;
  /* margin-top: 16rpx; *//* margin-bottom: 20rpx; */
  color: rgba(153, 153, 153, 1);
}
/* 分享赚 */
.style1 .goodlist .info .bto .left .sharget {
  min-width: 158rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  font-size: 22rpx;
  background: rgba(255, 43, 51, 1);
  border-radius: 4rpx;
  color: #fff;
  margin-bottom: 10rpx;
  padding: 0 10rpx;
}

/* 商品价格区 */
.style1 .goodlist .info  .pricebox {
  width: 100%;
  position: absolute;
  bottom: -6rpx;
}

.style1 .goodlist .info  .pricebox .nowprice {
  color: #ff2b33;
  font-size: 33rpx;
  font-weight: bold;
  margin-right: 15rpx;
}

.style1 .goodlist .info  .pricebox .befprice {
  font-size: 18rpx;
  color: rgba(162, 162, 162, 1);
}

/* 优惠券 */
.style1 .goodlist .info .bto .right {
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  color: rgba(255, 43, 51, 1);
  display: flex;
  flex-direction: column;
  align-items: center;
}

.style1 .goodlist .info .bto .right .coupons {
  width: 106rpx;
  height: 36rpx;
  line-height: 36rpx;
  color: #fff;
  text-align: center;
  margin-top: 60rpx;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 106rpx 36rpx;

}

.style1 .goodlist .info .bto .right .share_ico {
  display: block;
  width: 58rpx;
  height: 62rpx;
  background: orange;
  margin-top: 30rpx;
  margin-bottom: 8rpx;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 58rpx 62rpx;
}

/* 商品列表   样式2----------------------------------------------------------------------------*/

.style2 {
  display: flex;
  flex-wrap: wrap;
  padding: 0 24rpx;
}

.style2 .goodlist {
  width: 342rpx;
  min-height: 590rpx;
  height: auto;
  display: flex;
  margin-right: 18rpx;
  flex-direction: column;
  border: none !important;
  outline: none !important;
  background: #fff;
  margin-top: 10rpx;
  border-radius: 10rpx 10rpx 0px 0px;
  overflow: hidden;
}

.style2 .goodlist:nth-of-type(2n) {
  margin-right: 0rpx;
}

/* 商品图片 */
.style2 .goodlist .img {
  width: 350rpx;
  height: 350rpx;
  display: block;
  /* background: pink; */
}

/* 商品信息 */

.style2 .goodlist .info {
  flex: 1;
  padding: 0 14rpx;
  display: flex;
  flex-direction: column;
  position: relative;
}

/* 商品名 */
.style2 .goodlist .info .name {
  font-size: 26rpx;
  color: rgba(0, 0, 0, 1);
  /* overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; */
  overflow: hidden;
  word-break: break-all;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  margin-top: 25rpx;
  margin-bottom: 15rpx;
}

.style2 .goodlist .info .bto {
  width: 100%;
}

/* 商品店铺 */
.style2 .goodlist .info .bto  .top {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
}



.style2 .goodlist .info .bto .moddel {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: space-between;
  margin: 8rpx 0;
}

/* 商品销量 */
.style2 .goodlist .info .bto .moddel .left .sale {
  font-size: 22rpx;
  color: rgba(153, 153, 153, 1);
}

/* 优惠券*/
.style2 .goodlist .info .bto .moddel .right {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.style2 .goodlist .info .bto .right .coupons {
  width: 124rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: #fff;
  text-align: center;
  background: url('data:image/png;base64,') center center no-repeat;
  background-size: 106rpx 36rpx;
  /* margin-top: 70rpx; */
}

.style2 .goodlist .info .bto .sharget{
  width: 158rpx;
  height: 36rpx;
  line-height: 36rpx;
  text-align: center;
  font-size: 22rpx;
  background: rgba(255, 43, 51, 1);
  border-radius: 4rpx;
  color: #fff;
  margin-bottom: 10rpx;
}


/* 商品价格区 */

.style2 .goodlist .info  .pricebox {
  width: 100%;
  margin-bottom: 15rpx;
}

.style2 .goodlist .info  .pricebox .nowprice {
  color: #ff2b33;
  font-size: 33rpx;
  font-weight: bold;
  margin-right: 15rpx;
}

.style2 .goodlist .info   .pricebox .befprice {
  font-size: 18rpx;
  color: rgba(162, 162, 162, 1);
}
