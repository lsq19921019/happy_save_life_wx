/* 商品列表   样式1----------------------------------------------------------------------------*/
image {
  width: 100%;
  height: 100%;
}

.style1 .goodlist {
  width: 100%;
  height: 310rpx;
  display: flex;
  flex-wrap: nowrap;
  border: none !important;
  outline: none !important;
  padding: 24rpx;
  background: #fff;
  margin-top: 10rpx;
}

/* 商品图片 */

.style1 .goodlist .img {
  width: 270rpx;
  height: 270rpx;
  display: block;
  margin-right: 30rpx;
  position: relative;
}

.style1 .goodlist .img .toprank {
  position: absolute;
  top: 0;
  left: 15rpx;
  width: 50rpx;
  height: 60rpx;
  color: #fff;
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
}

.style1 .goodlist:nth-of-type(1) .img .toprank {
  background: url('https://bnlnimg.bnln100.com/toprank1.png') center center no-repeat;
  background-size: 50rpx 60rpx;
}

.style1 .goodlist:nth-of-type(2) .img .toprank {
  background: url('https://bnlnimg.bnln100.com/toprank2.png') center center no-repeat;
  background-size: 50rpx 60rpx;
}

.style1 .goodlist:nth-of-type(3) .img .toprank {
  background: url('https://bnlnimg.bnln100.com/toprank3.png') center center no-repeat;
  background-size: 50rpx 60rpx;
}

