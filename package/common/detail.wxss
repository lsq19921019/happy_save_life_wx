/*服务说明开始  */

.server {
  position: fixed;
  top: 0;
  left: 0;
  height: 1250rpx;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 111;
}

.servarBox {
  position: absolute;
  bottom: 0;
  left: 0;
  height: 70%;
  width: 100%;
  background-color: white;
}

.serverHead {
  text-align: center;
  line-height: 100rpx;
  width: 100%;
  font-size: 36rpx;
  font-weight: 800;
  border-bottom: 2rpx solid #f0f0f0;
  position: relative;
}

.uncheck {
  display: block;
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  top: 50%;
  right: 15rpx;
  transform: translateY(-50%);
  z-index: 11;
}

.serverHead::after {
  position: absolute;
  content: "";
  width: 5rpx;
  height: 40rpx;
  top: 50%;
  right: 30rpx;
  background-color: #909090;
  transform: translateY(-50%) rotate(45deg);
}

.serverHead::before {
  position: absolute;
  content: "";
  width: 40rpx;
  height: 5rpx;
  top: 50%;
  right: 12rpx;
  background-color: #909090;
  transform: translateY(-50%) rotate(45deg);
}

.serverDetails {
  margin: 20rpx auto 0;
  width: 95%;
  height: 100%;
}

.serverList {
  position: relative;
  height: 100rpx;
  width: 100%;
  margin: 30rpx auto 0;
  font-size: 24rpx;
}

.package {
  display: block;
  font-size: 32rpx;
  font-weight: 600;
  padding-bottom: 10rpx;
  letter-spacing: 3rpx;
}

.packageAll {
  display: block;
  color: #909090;
}

.serverPng {
  position: absolute;
  top: 0;
  left: 40rpx;
  width: 45rpx;
  height: 45rpx;
}

.serverPng image {
  width: 100%;
  height: 100%;
}

.serverInfo {
  width: 85%;
  height: 100%;
  margin-left: 110rpx;
}

