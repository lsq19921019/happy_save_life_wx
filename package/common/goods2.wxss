/* 弹窗 */
.withdraw{
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 999;
}
.withdraw .drawType{
  height: 280rpx;
  width: 460rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  background-color: white;
  border-radius: 12rpx;
}
.withdraw .drawType .drawHead{
  text-align: center;
  font-size: 36rpx;
  padding: 30rpx 0 20rpx;
}
.withdraw .drawType .sysType{
  text-align: center;
  color: #666666;
  font-size: 32rpx;
}
.withdraw .drawType .system{
  display: block;
  padding: 10rpx 0;
  font-size: 30rpx;
  
}
.withdraw .drawType .sysTip .logoImg{
  height: 30rpx;
  width: 30rpx;
}
.withdraw .drawType .sysTip .logoImg image{
  height: 100%;
  width: 100%;
}
.withdraw .drawType .sysTip{

   height: 30rpx; 
  line-height: 30rpx;
   font-size: 23rpx; 
}
.withdraw .drawType .iknow{
  display:flex;
  justify-content:space-around;
  color:#333;
  height: 80rpx;
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  border-top: 2rpx solid #f5f5f5;
  line-height: 80rpx;
  text-align: center;
  font-size: 26rpx;
}
.withdraw .drawType .iknow button{
  margin:0;
  padding:0;
  color:none;
  border:none;
}

.goodswrap{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}

.bottom{
  width: 100%;
}

.goodsbox.mode3-1-0-5:nth-last-of-type(2n+1) {
  margin-left:0;
}

.goodsbox.mode3-1-0-5:nth-child(2n) {
  margin-left:14rpx;
}