/* package/pages/merchants/merchants.wxss */

.commList{
  margin-top: 120rpx;
}
.Oflex{
  position: fixed;
  top: 60rpx;
  left: 0rpx;
  z-index: 1100;
}
.seekbox {
  width: 100%;
  padding: 0 30rpx;
  height: 60rpx;
  position: fixed;
  margin: 0 auto;
  display: flex;
  line-height: 60rpx;
  top: 0;
  left: 0;
  background-color: white;
  z-index: 1100;
}

 .seekbox .seekbug{
  width:28rpx;
  height:28rpx;
  position:absolute;
  left:48rpx;
  top:18rpx;
  z-index:20;
  background:url("data:image/png;base64,") no-repeat; 
  background-size: 30rpx 30rpx;
}

 .seekbox input {
  position: relative;
  height: 60rpx;
  width: 500rpx;
  line-height: 60rpx;
  padding: 0 60rpx 0 64rpx;
  background: #f2f2f2;
  border-radius: 50rpx;
}

 .seekbox .delete {
  position: absolute;
  width: 34rpx;
  height: 34rpx;
  right: 18rpx;
  top: 14rpx;
  z-index: 20;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 34rpx;
}

 .seekbox .seekGo {
  width: 96rpx;
  height: 54rpx;
  margin-top: 2rpx;
  margin-left: 20rpx;
  line-height: 54rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size:96rpx 54rpx;
  border-radius: 100rpx;
  color: #fff;
  text-align: center;
}


