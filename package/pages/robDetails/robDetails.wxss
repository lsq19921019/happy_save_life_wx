/* 视频播放 */

.details .play {
  position: absolute;
  bottom: 104rpx;
  left: 30rpx;
  width: 80rpx;
  height: 80rpx;
  z-index: 9;
}

.details .hint {
  position: absolute;
  width: 100%;
  text-align: center;
  background: rgba(0, 0, 0, 0.5);
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  bottom: 0;
}

/* 分享图+转发好友 */

.button {
  position: fixed;
  top: 80rpx;
  right: 0;
  border: none !important;
  padding: 0 !important;
}

.lastButton {
  top: 216rpx !important;
}

.button::after {
  width: 0 !important;
  height: 0 !important;
}

.button image {
  width: 130rpx;
  height: 58rpx;
}

/* 图片 */

.details .detailImage {
  width: 100%;
  /* margin-top:-136rpx; */
}

/* 轮播图 */

.details .swiper {
  height: 750rpx;
  width: 100%;
  /* margin-top:-136rpx; */
}

.details .swiper image {
  height: 100%;
  width: 100%;
}

.details .goods_info  .disTicket {
  width: 95%;
  height: 130rpx;
  margin: 0 auto;
  position: relative;
}

.details .goods_info  .disTicket image {
  width: 100%;
  height: 100%;
  z-index: 999;
}

.details .goods_info .now {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 20rpx;
  color: white;
  width: 24%;
  height: 100%;
  font-size: 34rpx;
  line-height: 130rpx;
}

.details .goods_info .yuan {
  position: absolute;
  top: 58%;
  transform: translate(-50%, -50%);
  left: 30%;
  color: white;
  font-size:36rpx;
  height: 50%;
  text-align: center;
  width: 80%;
}
.goods_info .participant {
  margin-bottom: 20rpx;
  color: #666;
  overflow: hidden;
  white-space: nowrap;
  background:#fff;
  margin-top:20rpx;
}

.goods_info .participant>p {
  margin-right: 30rpx;
  font-size: 28rpx;
  padding: 20rpx 0 10rpx 26rpx;
}

.goods_info .participant .pSwiper view {
  display: inline-block;
  text-align: center;
  width: 148rpx;
}

.goods_info .participant .pSwiper {
  display: inline-block;
  width: 100%;
  margin-left: 20rpx;
  min-height: 20rpx;
}

.goods_info .participant .pSwiper image {
  width: 102rpx;
  height: 102rpx;
  border-radius: 50%;
}

.goods_info .participant .pSwiper p {
  font-size: #666;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 110rpx;
  padding-top: 12rpx;
  margin: 0 auto;
}

.goods_info .detailImg {
  border-top: 2rpx solid #ececec;
  border-bottom: 2rpx solid #ececec;
  font-size: 28rpx;
  margin: 0 30rpx;
  padding: 18rpx 0;
  color: #666;
  background: url("data:image/png;base64,") no-repeat 96%;
  background-size: 21rpx 11rpx;
}

.goods_info .downArrow {
  background: url("data:image/png;base64,") no-repeat 96%;
  background-size: 21rpx 11rpx;
}

.goods_info .detailImg image {
  height: 30rpx;
  width: 30rpx;
  float: right;
}

/* 底部样式一 */

.operation {
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  background: #fff;
  position: fixed;
  padding: 10rpx;
  border-top: 2rpx solid #e5e4e4;
  text-align: center;
}

.operation .common {
  font-size: 32rpx;
  overflow: visible;
  border-radius: 50rpx;
  margin: 0 auto;
  padding-left: 0;
  padding-right: 0;
  border: none !important;
  width: 630rpx;
  color: #fff;
  line-height: 80rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 630rpx 80rpx;
}

.operation .accomplish {
  background: #cecece;
  text-align: center;
}

.operation .help {
  font-size: 24rpx;
  color: #666;
  line-height: 74rpx;
  margin:0 40rpx;
}

.operation .help image {
  width: 82rpx;
  height: 82rpx;
  border-radius: 50%;
  margin-right: 24rpx;
  float: left;
}

.operation .help text {
  float: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 160rpx;
  font-size: 26rpx;
  text-align: left;
}

.operation .help p {
  float: right;
  width: 370rpx;
  height: 80rpx;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100%;
}

.bottom {
  margin: 16rpx 0;
  text-align: center;
  color: #777;
}

.imgMark {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
  z-index: 21;
  text-align: center;
}

.imgMark image {
  width: 60%;
  height: 60%;
  position: absolute;
  left: 50%;
  top: 70rpx;
  transform: translate(-50%, 0);
}

.imgMark>view {
  color: #fff;
  position: absolute;
  top: 71%;
  width: 100%;
  font-size: 14px;
}

.imgMark>.text_t {
  top: 76%;
}

.imgMark>button {
  width: 300rpx;
  position: absolute;
  top: 85%;
  left: 50%;
  transform: translate(-50%, 0);
}

.menuBar {
  top: 260px;
  bottom: auto;
  width: 35px;
  height: 35px;
  right: 14rpx;
  background: none rgba(0, 0, 0, 0.6);
  border-radius: 100%;
}

.menuBar.cur {
  background: none rgba(0, 0, 0, 0.8);
}


/* 己完成砍价样式 */

.operation .shareApp {
  font-size: 32rpx;
  overflow: visible;
  border-radius: 50rpx;
  padding-left: 0;
  padding-right: 0;
  border: none !important;
  color: #fff;
  width:88%;
  float:right;
}

.operation  .shareApp .sa {
  display: inline-block;
  width: 50%;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.operation  .shareApp .sa:nth-child(2) {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 80rpx;
}

.operation .shareApp .sa:nth-child(1) {
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 80rpx;
  overflow: visible;
  border-radius: 0;
  margin-left: 0;
  margin-right: 0;
  padding-left: 0;
  padding-right: 0;
  border: none;
}

